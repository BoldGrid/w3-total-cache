{
    "require": {
        "aws/aws-sdk-php": "3.183.8",
        "aws/aws-php-sns-message-validator": "^1.8",
        "bunnyway/bunnycdn.php.storage": "dev-master"
    },
    "require-dev": {
        "yoast/phpunit-polyfills": "^1.0",
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "wp-coding-standards/wpcs": "dev-develop"
    },
    "replace": {
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-mbstring": "*"
    },
    "repositories": [
        {
          "type": "package",
          "package": {
            "name": "bunnyway/bunnycdn.php.storage",
            "version": "master",
            "type": "vcs",
            "source": {
              "url": "https://github.com/BunnyWay/BunnyCDN.PHP.Storage.git",
              "type": "git",
              "reference": "master"
            }
          }
        }
      ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "optimize-autoloader": true,
        "COMPOSER_MIRROR_PATH_REPOS": true
    }
}
